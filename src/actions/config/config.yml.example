## Bridge Configuration
stathat_key: "update_me"
envname: "[Dev-Fork]"
dbqueue: dc1queue
sleep: 30
## Redis Config
redis_host: redis_ip
redis_port: 3679
redis_db: 0
redis_password: False
## Rethink Config
rethink_host: rethink_host
rethink_port: 28015
rethink_authkey: False
rethink_db: "crdb"
## Sink Config
sink_ip: sink_host
sink_port: 6000
sink_worker_port: 6001
## Maximum time for monitor execution
max_monitor_time: 600
default_actions:
  - logit-monitor
  - logit-events
  - chstatus
  - timetracking
reaction_meta:
  - logit-reaction
  - update-lastrun
## History Retention
history_retention: 18000
## MailChimp
mailchimp_api_key: "update_me!"
mailchimp_list_id: "update_me!"
mailchimp_api_url: "update_me!"
## Mandrill
mandrill_api_url: "https://mandrillapp.com/api/1.0/"
mandrill_api_key: "update_me!"
## Logging
use_syslog: False
